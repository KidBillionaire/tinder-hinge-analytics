# API Configuration
PORT=4000
HTTPS_PORT=4443
NODE_ENV=staging

# ML Model Configuration
ML_MODEL_ENDPOINT_STAGING=https://staging-ml.yourdomain.com
ML_API_KEY_STAGING=your_staging_api_key

# Database Configuration
DB_HOST_STAGING=staging-db.yourdomain.com
DB_USER_STAGING=staging_user
DB_PASSWORD_STAGING=staging_password
DB_NAME_STAGING=dating_assistant_staging

# Redis Configuration
REDIS_HOST_STAGING=staging-redis.yourdomain.com
REDIS_PORT_STAGING=6379
REDIS_PASSWORD_STAGING=staging_redis_password

# SSL Configuration
SSL_KEY_PATH_STAGING=/etc/letsencrypt/live/staging.yourdomain.com/privkey.pem
SSL_CERT_PATH_STAGING=/etc/letsencrypt/live/staging.yourdomain.com/fullchain.pem

# Security
JWT_SECRET_STAGING=your_staging_jwt_secret
SESSION_SECRET_STAGING=your_staging_session_secret
CORS_ORIGIN_STAGING=https://staging.yourdomain.com

# Monitoring
SENTRY_DSN_STAGING=your_sentry_dsn
NEW_RELIC_LICENSE_KEY_STAGING=your_newrelic_key

# OpenAI Configuration
OPENAI_API_KEY_STAGING=your_staging_openai_key 